function  vertex = updateVertex(u)

%single compute for contains
u_in_U = open.contains(u);

%check remove case first
if g == rhs && u_in_U
    open.remove(u);

%calculate key
u.calculateKey();

%remove if necessary
if g ~= rhs && u_in_U %fairly certain this just simplifies to if it's in u remove i

%insert


if g ~= rhs && u in U
    U.update(u,calculateKey(u));

else if g ~= rhs && u not in U
    U.insert(u,calculateKey(u));
else if g == rhs && u in U
   U.remove(u);         
end