classdef DSL_Node
    %DSL_NODE Summary of this class goes here
    %   Detailed explanation goes here
    
    properties
        parent
        x
        y
        g
        rhs
        Key1
        Key2
    end
    
    methods
        function obj = DSL_Node(inputArg1)
            %DSL_NODE Construct an instance of this class
            %   Things needed for a node... x, y, parent, g, rhs, Key1,
            %   Key2
            obj.g = inputArg1;
        end
        
        function boolout = gt(a,b)
            %overriding greater than
            %   compares keys
            ak1 = a.Key1;
            ak2 = a.Key2;
            bk1 = b.Key1;
            bk2 = b.Key2;
            if(ak1 == bk1)
                boolout = ak2 > bk2;
            else
                boolout = ak1 > bk1;
            end
        end
        
        function boolout = lt(a,b)
            %overriding less than
            %   compares keys
            if(a.Key1 == b.Key1)
                boolout = a.Key2 < b.Key2;
            else
                boolout = a.Key1 < b.Key1;
            end
        end
        
        function boolout = eq(a,b)
            %overriding equals
            %   compares keys
            boolout = a.Key1 == b.Key1 && a.Key2 == b.Key2;
        end

    end
end

